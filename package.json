{
  "name": "restaurant-pos",
  "version": "4.0.5",
  "description": "Simple restaurant point of sale system",
  "main": "./public/js/index.js",
  "html": [
    "./public/templates/login.html",
    "./public/templates/orders.html",
    "./public/templates/checkout.html",
    "./public/templates/tables.html"
  ],
  "mainHtml": "./public/templates/index.ejs",
  "ico": "./public/img/restaurant.ico",
  "css": "./public/css/index.less",
  "scripts": {
    "test": "mocha test/ --recursive",
    "jshint": "jshint src/. test/. --config",
    "start": "node src/",
    "mkdir": "mkdir -p build",
    "postinstall": "npm run clean && npm run mkdir && npm run build:webpack",
    "watch": "npm run watch:webpack",
    "clean": "rm -rf build",
    "build:webpack": "npm run build:webpack:unminified && npm run build:webpack:minified",
    "watch:webpack": "npm run watch:webpack:unminified & npm run watch:webpack:minified",
    "clean:webpack": "npm run clean:webpack:base npm run clean:webpack:unminified && npm run clean:webpack:minified",
    "clean:webpack:base" : "build/*.html build/*.js build/*.js.map",
    "build:webpack:unminified": "npm run clean:webpack:unminified && webpack",
    "watch:webpack:unminified": "npm run build:webpack:unminified && webpack --watch",
    "clean:webpack:unminified": "rm -f build/*.*.* build/*.*.*.map",
    "build:webpack:minified": "npm run clean:webpack:minified && webpack --minify",
    "watch:webpack:minified": "npm run build:webpack:minified && webpack --watch --minify",
    "clean:webpack:minified": "rm -f build/*.*.min.* build/*.*.min.*.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayzamazing/Meal-Calculator.git"
  },
  "keywords": [
    "restaurant",
    "pos",
    "food",
    "html",
    "css",
    "javascript",
    "feathersjs",
    "angular",
    "babel",
    "webpack",
    "nodejs",
    "passport"
  ],
  "author": "Adrian J Lopez <adrianjaylopez@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jayzamazing/Restaurant-POS/issues"
  },
  "homepage": "https://github.com/jayzamazing/Restaurant-POS#readme",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-route": "^1.5.8",
    "bcrypt": "0.8.7",
    "body-parser": "1.15.2",
    "bootstrap": "3.3.6",
    "compression": "1.6.2",
    "express": "4.14.0",
    "feathers": "2.0.1",
    "feathers-authentication": "0.7.9",
    "feathers-configuration": "0.2.3",
    "feathers-errors": "2.4.0",
    "feathers-hooks": "1.5.5",
    "feathers-localstorage": "^0.4.1",
    "feathers-mongoose": "^3.5.1",
    "feathers-rest": "1.4.3",
    "feathers-socketio": "1.4.1",
    "handlebars": "4.0.5",
    "jquery": "2.2.3",
    "mongoose": "4.5.8",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "serve-favicon": "2.3.0",
    "socket.io": "1.4.8",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-syntax-async-functions": "6.8.0",
    "babel-plugin-transform-regenerator": "6.11.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "chokidar-cli": "1.2.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "ejs-loader": "^0.3.0",
    "extract-loader": "0.0.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jshint": "^2.9.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "less-plugin-clean-css": "1.5.1",
    "mocha": "3.0.2",
    "request": "2.74.0",
    "style-loader": "^0.13.1",
    "uglify-js": "2.6.4",
    "webpack": "1.13.1"
  }
}
